#!/bin/bash

# Get the C file name from the command line argument
CFILE=$1

# Generate the output file name by replacing the extension .c with .s
OUTFILE="${CFILE%.c}.s"

# Compile the C code to assembly using gcc
gcc -S -o $OUTFILE $CFILE

# Print a message to indicate the process is complete
echo "Assembly code generated and saved in $OUTFILE"
